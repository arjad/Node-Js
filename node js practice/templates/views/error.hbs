    <style>
    h1{
        margin-top: 100px;
        margin-bottom: 100px;
        text-align: center;
    }
    .svg{
        margin:auto;
        width:20%;
    }
    *{
        margin:0px
    }
    header{
        top:0px;
        padding-top: 1%;
        padding-bottom: 3%;
        box-sizing: border-box;
        width:100%;
        background-image: linear-gradient(to left,rgb(64, 64, 243),rgb(3, 126, 167),blue,rgb(76, 76, 240));
        clip-path: polygon(0% 0%,100% 0%,100% 61%,0% 100%);
        position: fixed;
        z-index: 2;
    }
    header ul li{
        font-size: 20px;
        font-family: Arial, Helvetica, sans-serif;
        font-weight: bold;
        display: inline-block;
        margin-left: 5%;
        padding: 7px;
        box-sizing: border-box;
        transition-duration: .4s;
    }
    header ul li:nth-child(2)
    {
        background-color: white;
        color:blue;
    }
    header .fas{
        color: white;
        font-size: 35px;
        margin-left: 5%;
        display: inline-block;
    }
    header ul{
        color: white;
        height:100%;
        width:90%;
        margin:auto;
        list-style: none;
        display: inline-block;
    }
    header ul li:hover{
        color:blue;
        background:white;
        cursor: pointer;
    }
